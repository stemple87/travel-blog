@model IEnumerable<TravelBlog.Models.Location>

@{
ViewData["Title"] = "Travel Blog";
Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Travel Blog</h2>
<ul>
    @foreach (var listItem in Model)
    {
        <li>@Html.DisplayFor(modelItem => listItem.Name)</li>
    }
</ul>
<p>@Html.ActionLink("Add new location", "Create")</p>
